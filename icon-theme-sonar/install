cp /usr/share/icons/gnome/$i/mimetypes/video-x-generic.png ./$i/categories/emblem-video.png
cp /usr/share/icons/gnome/$i/apps/utilities-system-monitor.png ./$i/categories/icon-television.png
cp /usr/share/icons/gnome/$i/apps/preferences-desktop-locale.png ./$i/categories/edu_languages.png
cp /usr/share/icons/gnome/$i/apps/accessories-calculator.png ./$i/categories/edu_mathematics.png
cp /usr/share/icons/gnome/$i/categories/applications-other.png ./$i/categories/edu_miscellaneous.png
cp /usr/share/icons/gnome/$i/categories/applications-science.png ./$i/categories/edu_science.png
cp /usr/share/icons/gnome/$i/mimetypes/x-office-address-book.png ./$i/categories/icon-logbook.png
